                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT prodD.id AS ProductoID, cliD.id AS ClienteID,empD.id AS EmpleadoID,tiD.id AS TiempoID,provD.id AS ProveedorID,detoN.Quantity*detoN.UnitPrice AS PrecioTotal,detoN.Quantity AS Cantidad
FROM Empresa.dbo.Products prodN INNER JOIN Empresa.dbo.Categories catN ON (prodN.CategoryID=catN.CategoryID)
                                INNER JOIN Empresa.dbo.Suppliers provN ON (prodN.SupplierID=provN.SupplierID)
                                INNER JOIN Empresa.dbo.[Order Details] detoN ON (prodN.ProductID=detoN.ProductID)
								INNER JOIN Empresa.dbo.Orders ordN ON (ordN.OrderID=detoN.OrderID)
