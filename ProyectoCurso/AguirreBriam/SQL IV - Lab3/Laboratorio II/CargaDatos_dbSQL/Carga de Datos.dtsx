 								INNER JOIN Empresa.dbo.Employees empN ON  (empN.EmployeeID=ordN.EmployeeID)
								INNER JOIN dbSQL.dbo.DimProducto prodD ON (prodD.nombre=prodN.ProductName)
								INNER JOIN dbSQL.dbo.DimCliente cliD ON (cliD.nombre=cliN.ContactName)
								INNER JOIN dbSQL.dbo.DimEmpleado empD ON (empD.apellido=empN.LastName)
								INNER JOIN dbSQL.dbo.DimTiempo tiD ON (tiD.fecha=CONVERT(DATE,ordN.OrderDate))
								INNER JOIN dbSQL.dbo.DimProveedor provD ON (provD.nombreCompania=provN.CompanyName)</property>
